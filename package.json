{
  "name": "lemna",
  "version": "1.0.0",
  "description": "Quickly scaffold and deploy AWS Lambda handlers powered by Javascript or Typescript",
  "main": "dist/bin.js",
  "bin": {
    "lemna": "cli.js"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "ava -c 1 test/**/*.spec.ts",
    "coverage": "cross-env LEMNA_LOG_LEVEL=debug nyc npm test",
    "postcoverage": "nyc report --reporter=json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/lemna.git"
  },
  "keywords": [
    "lambda",
    "aws",
    "scaffold"
  ],
  "author": "marvin-j97",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marvin-j97/lemna/issues"
  },
  "homepage": "https://github.com/marvin-j97/lemna#readme",
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/glob-parent": "^5.1.1",
    "@types/inquirer": "^8.1.3",
    "@types/node": "^16.3.2",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "ava": "^3.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist",
    "cli.js"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "dependencies": {
    "@dotvirus/yxc": "^2.0.2",
    "aws-sdk": "^2.1058.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.11",
    "glob": "^7.1.6",
    "glob-parent": "^6.0.2",
    "inquirer": "^8.2.0",
    "jszip": "^3.6.0",
    "winston": "^3.3.3",
    "yargs": "^17.0.1"
  }
}
