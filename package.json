{
  "name": "lemna",
  "version": "2.0.0",
  "description": "Quickly scaffold and deploy AWS Lambda handlers powered by Javascript or Typescript",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "bin": {
    "lemna": "cli.js"
  },
  "engines": {
    "node": ">=14.18"
  },
  "license": "GPL-3.0",
  "scripts": {
    "start": "node cli",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "lint": "eslint src --ext .ts",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/lemna.git"
  },
  "keywords": [
    "lambda",
    "aws",
    "scaffold",
    "bundling",
    "esbuild",
    "serverless"
  ],
  "author": "marvin-j97",
  "bugs": {
    "url": "https://github.com/marvin-j97/lemna/issues"
  },
  "homepage": "https://github.com/marvin-j97/lemna#readme",
  "devDependencies": {
    "@types/glob": "7.1.4",
    "@types/glob-parent": "5.1.1",
    "@types/inquirer": "7.3.3",
    "@types/node": "16.3.2",
    "@types/yargs": "17.0.0",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "@vitest/coverage-istanbul": "0.26.2",
    "cross-env": "7.0.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsdoc": "39.6.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "prettier": "2.8.1",
    "tsup": "6.5.0",
    "typescript": "4.9.4",
    "vitest": "0.29.8"
  },
  "files": [
    "dist",
    "cli.js"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "aws-sdk": "2.1286.0",
    "chalk": "5.2.0",
    "dotenv": "16.0.3",
    "esbuild": "0.16.12",
    "glob": "7.1.6",
    "glob-parent": "6.0.2",
    "inquirer": "7.3.3",
    "jszip": "3.8.0",
    "skriva": "0.0.2",
    "skriva-transport-console": "0.0.2",
    "yargs": "17.0.1",
    "zod": "3.20.2"
  }
}
