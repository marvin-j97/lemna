{
  "name": "lemna",
  "version": "4.0.0",
  "description": "Quickly scaffold and deploy AWS Lambda handlers powered by Typescript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "bin": {
    "lemna": "cli.mjs"
  },
  "engines": {
    "node": "18 || 20 || 22"
  },
  "license": "MIT",
  "scripts": {
    "start": "node cli.mjs",
    "dev": "tsx src/bin.ts",
    "tc": "tsc",
    "build": "tsup --entry src/bin.ts --entry src/index.ts --format esm --dts --clean",
    "biome:check": "biome check src",
    "lint": "biome lint src",
    "lint:fix": "biome lint --apply src",
    "fmt": "biome format src --write",
    "fmt:check": "biome format src",
    "test": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/lemna.git"
  },
  "keywords": [
    "lambda",
    "aws",
    "scaffold",
    "bundling",
    "esbuild",
    "serverless"
  ],
  "author": "marvin-j97",
  "bugs": {
    "url": "https://github.com/marvin-j97/lemna/issues"
  },
  "homepage": "https://github.com/marvin-j97/lemna#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/glob-parent": "5.1.3",
    "@types/inquirer": "9.0.7",
    "@types/node": "22.8.4",
    "@types/yargs": "17.0.33",
    "@vitest/coverage-istanbul": "2.1.4",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "vitest": "2.1.4"
  },
  "files": [
    "dist",
    "cli.js"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "3.682.0",
    "chalk": "5.3.0",
    "dotenv": "16.4.5",
    "esbuild": "0.24.0",
    "glob": "11.0.0",
    "glob-parent": "6.0.2",
    "inquirer": "12.0.1",
    "jszip": "3.10.1",
    "skriva": "0.0.2",
    "skriva-transport-console": "0.0.2",
    "yargs": "17.7.2",
    "zod": "3.24.2"
  }
}