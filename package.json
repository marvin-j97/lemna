{
  "name": "lemna",
  "version": "0.1.0",
  "description": "Quickly scaffold and deploy AWS Lambda handlers powered by Javascript or Typescript",
  "main": "dist/bin.js",
  "bin": {
    "lemna": "cli.js"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --fix --ext .ts",
    "test": "ava -c 1 test/**/*.spec.ts",
    "coverage": "nyc npm test",
    "postcoverage": "nyc report --reporter=json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marvin-j97/lemna.git"
  },
  "keywords": [],
  "author": "marvin-j97",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marvin-j97/lemna/issues"
  },
  "homepage": "https://github.com/marvin-j97/lemna#readme",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^15.0.1",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "ava": "^3.15.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist",
    "cli.js"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ]
  },
  "dependencies": {
    "@dotvirus/yxc": "^2.0.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "aws-sdk": "^2.860.0",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "jszip": "^3.6.0",
    "rollup": "^2.41.0",
    "rollup-plugin-uglify": "^6.0.4",
    "winston": "^3.3.3",
    "yargs": "^17.0.0"
  }
}
